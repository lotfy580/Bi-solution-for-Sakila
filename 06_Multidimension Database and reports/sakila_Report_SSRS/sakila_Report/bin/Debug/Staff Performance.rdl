  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>sakilaUS</DataSourceName>
        <CommandText>SELECT 
	st.staffId,
	st.firstName+' '+st.lastName AS FullName,
	ad.address,
	st.email,
	py.amount,
	MONTH(py.paymentDate) as month

FROM Staff st inner join Address ad
ON st.addressId = ad.addressId inner join Payment py
ON py.staffId = st.staffId 
WHERE YEAR(py.paymentDate) = 2020</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="staffId">
          <DataField>staffId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="address">
